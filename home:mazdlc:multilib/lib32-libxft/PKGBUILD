_pkgbase=libxft
pkgname=lib32-libxft
pkgver=2.3.2
pkgrel=1
pkgdesc="32bit library for obs"
arch=('x86_64')
depends=( lib32-fontconfig lib32-libxrender )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}