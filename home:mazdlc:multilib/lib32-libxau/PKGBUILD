_pkgbase=libxau
pkgname=lib32-libxau
pkgver=1.0.8
pkgrel=1
pkgdesc="32bit library for obs"
arch=('x86_64')
depends=( lib32-glibc libxau )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}