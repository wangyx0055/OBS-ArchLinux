_pkgbase=pcre
pkgname=lib32-pcre
pkgver=8.37
pkgrel=2
pkgdesc="32bit library for obs"
arch=('x86_64')
depends=( lib32-gcc-libs pcre )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}