pkgname=PKGNAME
pkgver=VERSION
pkgrel=1
pkgdesc="package for obs"
arch=('i686')
depends=( DEPENDENCY )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}