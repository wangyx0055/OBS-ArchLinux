pkgname=zeromq
pkgver=4.1.3
pkgrel=1
pkgdesc="package for obs"
arch=('i686')
depends=( gcc-libs libpgm libsodium util-linux )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}