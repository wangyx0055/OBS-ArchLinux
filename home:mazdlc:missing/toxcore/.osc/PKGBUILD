pkgname=toxcore
pkgver=3661
pkgrel=1
pkgdesc="package for obs"
arch=('x86_64')
depends=( systemd libconfig libsodium libvpx opus )
provides=( tox )
conflicts=( tox )
url="http://www.archlinux.org"
license=('see original')
source=("pkg.tar.xz")
md5sums=('SKIP')
noextract="pkg.tar.xz"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/pkg.tar.xz"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}