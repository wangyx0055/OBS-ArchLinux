pkgname=cwiid
pkgver=0.6.00+svn201
pkgrel=1
pkgdesc="package for obs"
arch=('x86_64')
depends=( bluez-libs gtk2 python2 )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}