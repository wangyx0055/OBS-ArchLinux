pkgname=cdemu-daemon
pkgver=3.0.2
pkgrel=2
pkgdesc="package for obs"
arch=('x86_64')
depends=( dbus libao libmirage )
url="http://www.archlinux.org"
license=('see original')
source=("index.html")
md5sums=('SKIP')
noextract="index.html"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/index.html"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}