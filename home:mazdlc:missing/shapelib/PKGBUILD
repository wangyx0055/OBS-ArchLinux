pkgname=shapelib
pkgver=1.3.0
pkgrel=5
pkgdesc="package for obs"
arch=('x86_64')
depends=(  )
provides=(  )
conflicts=(  )
url="http://www.archlinux.org"
license=('see original')
source=("pkg.tar.xz")
md5sums=('SKIP')
noextract="pkg.tar.xz"


package() {
	cd ${pkgdir}
	tar xvpJf "$startdir/pkg.tar.xz"
	rm ./.MTREE
	rm ./.PKGINFO
	rm -f ./.INSTALL
}